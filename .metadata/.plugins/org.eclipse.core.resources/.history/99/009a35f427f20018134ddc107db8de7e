package testProject.test;

public class AddProgram {
	
	public static void main(String args[])
	{
		
		int n=6;
		int[] a = new int[30];
		
		for(int i=0;i<n;i++)
		{
			a[i]=i+1;
		}
		for(int i=0;i<n;i++)
		{
			System.out.println(a[i]);
		}
		
		System.out.println("===========================");
		for(int i=0;i<n;i++)
		{
			System.out.println(a[i]+i);
		}
		
		
		
		
		
	}

}
