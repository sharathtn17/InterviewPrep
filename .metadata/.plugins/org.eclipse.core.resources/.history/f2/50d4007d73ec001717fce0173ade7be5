package Profram;

public class PrintNameinStar {
	public static void main(String[] args) {
		int n=6;
		for(int i=0;i<=n;i++)
		{  
			System.out.print(" ");
			for(int j=0;j<i;j++)
			{
				 System.out.println("*");
			}
			
		}
		
	}

}

