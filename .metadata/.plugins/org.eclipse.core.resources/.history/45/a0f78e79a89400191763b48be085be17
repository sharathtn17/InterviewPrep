package dataStructure;

public class LinkedList {
    
	static Node head;
	
	
	static  class Node
	{
		int data;
		Node next;
		
		public static void addToList(int element)
		{   
			Node newnode=new Node();
			if(head==null)
			{
				newnode.data=element;
				newnode.next=null;
			}
				Node temp=head;
			while(temp.next!=null)
			{   
				temp=temp.next;
			}
			
			temp.next=newnode;
			newnode.data=element;
			newnode.next=null;
		}
		
	}
	 
	 
}
