package testProject.test;

public class RotationArray {
	
	
	public static void main(String args[])
	{
		
		int arr[]={1,2,3,4,5,6,7};
		boolean IsClockWise=true;
		int rotation=1;
		for(int i=0;i<arr.length;i++)
		{
			int temp=arr[0];
			arr[0]=arr[arr.length];
			arr[arr.length]=temp;
			
		}
		
		for(int i=0;i<arr.length;i++)
		{
			System.out.print(arr[i]);
		}
		
	}

}
