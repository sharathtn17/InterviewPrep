package testProject.test;

public class stringManupilation {
	
	
	public static void main(String args[])
	{
		
		String s1="abcd";
		String s2="ef";
		String s3="";
		
		if(s1.length()>=s2.length())
		{   //int i=s1.length();
			for( int i=0:i<s1.length();i++)
			{
				s3=s3+s1.charAt(i)+s2.charAt(i)+"";
				//i++;
			}
			
			System.out.println(s3);
		}
		
		
	}

}
